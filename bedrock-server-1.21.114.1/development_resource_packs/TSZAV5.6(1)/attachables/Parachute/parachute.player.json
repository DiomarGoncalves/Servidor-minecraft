{
   "format_version":"1.10.0",
   "minecraft:attachable":{
      "description":{
         "identifier":"true:parachute.player",
         "item":{
            "true:parachute":"query.owner_identifier == 'minecraft:player'"
         },
         "materials":{
            "default":"armor",
            "enchanted":"armor_enchanted"
         },
         "textures":{
            "default":"textures/parachute/parachute",
            "enchanted":"textures/misc/enchanted_item_glint"
         },
         "geometry":{
            "default":"geometry.parachute"
         },
         "animations":{
            "open":"animation.parachute.open",
            "closed":"animation.parachute.closed"
         },
         "scripts":{
            "parent_setup":"variable.chestplate_layer_visible = 0.0;",
            "animate":[
               {
                  "open":"!query.is_in_water && !query.is_on_ground"
               },
               {
                  "closed":"!query.is_in_water && query.is_on_ground"
               }
            ]
         },
         "render_controllers":[
            "controller.render.armor"
         ]
      }
   }
}